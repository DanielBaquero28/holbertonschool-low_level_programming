#include "holberton.h"
#include <string.h>

/**
 * reverse_array - Compares two strings
 * @s1: Pointer that links with a string
 * @s2: Pointer that links with a string
 *
 * Return: Nothing
**/
void reverse_array(int *a, int n)
{
  int i;
  int tem;
  i = 0;
  n = n - 1;
  while (i < n)
    {
      tem = a[i];
      a[i] = a[n];
      a[n] = tem;
      i++;
      n--;
}
}
