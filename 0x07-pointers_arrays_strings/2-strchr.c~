#include "holberton.h"
#include <stdlib.h>

/**
 * _strchr - Function that copies memory area.
 * @s: Pointer
 * @c: Character that starts from in order to print.
 * Return: A pointer to dest.
**/
char *_strchr(char *s, char c)
{
int i = *s;

while (i != '\0')
{
      if (i == c)
	{
	  return (s);
      i++;
	}
}
if (i == c)
return (s);
else
return (NULL);
}
